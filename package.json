    "@types/jest": "^28.1.6",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-plazide": "^0.1.6",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "open": "^8.4.0",
